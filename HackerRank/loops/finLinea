import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
 
    Scanner linea = new Scanner(System.in);
    int i=1;
    
    
    while(linea.hasNext())
    {
        String texto = linea.nextLine();
        
        System.out.print(i + " " + texto + "\n");
        
        i++;
    }
    
    
    linea.close();
  }
}
